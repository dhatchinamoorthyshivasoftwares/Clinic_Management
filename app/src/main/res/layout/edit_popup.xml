<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#f0f4f7"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/llay"
        android:orientation="horizontal"
        android:background="#2a8577">
        <TextView
            android:id="@+id/title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="18dp"
            android:textStyle="bold"
            android:padding="12dp"
            android:drawablePadding="12dp"
            android:textColor="@android:color/white"
            android:layout_weight="0.9" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/close"
            android:padding="5dp"
            android:src="@drawable/ic_baseline_close_24"
            android:layout_gravity="center"
            android:layout_marginRight="5dp"
            android:layout_weight="0.1"/>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/LL_booking_screen"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:visibility="visible"
        android:weightSum="2">

        <androidx.cardview.widget.CardView
            android:id="@+id/collection_cv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="0dp"
            app:cardBackgroundColor="#f2fcfc"
            app:cardCornerRadius="0dp"
            app:cardElevation="6dp"
            app:cardMaxElevation="8dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="false">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:gravity="top"
                android:orientation="vertical"
                android:padding="2dp"
                android:weightSum="0">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginBottom="1dp"
                    android:background="@drawable/search_background"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="2dp"
                    android:visibility="gone"
                    android:weightSum="2">

                    <EditText
                        android:id="@+id/main_search_edittext"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginEnd="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_marginBottom="2dp"
                        android:layout_weight="1.40"
                        android:background="@android:color/transparent"
                        android:digits="0123456789qwertzuiopasdfghjklyxcvbnm"
                        android:gravity="left|center_vertical"
                        android:hint="Search Name,ID"
                        android:inputType="text"
                        android:maxLines="1"
                        android:padding="0dp"
                        android:singleLine="true"
                        android:textColor="#222c36"
                        android:textColorHint="@color/gray"
                        android:textSize="20dp"
                        android:textStyle="normal"
                        android:visibility="visible">

                    </EditText>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.60"
                        android:background="@drawable/search_background"
                        android:gravity="end"
                        android:orientation="horizontal"
                        android:padding="0dp"
                        android:visibility="visible">

                        <ImageView
                            android:layout_width="45dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="0dp"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="0dp"
                            android:layout_marginRight="0dp"
                            android:layout_weight="0"
                            android:background="@drawable/search_clear"
                            android:gravity="center"
                            android:padding="0dp"
                            android:visibility="gone" />

                        <ImageView
                            android:layout_width="45dp"
                            android:layout_height="45dp"
                            android:layout_marginStart="0dp"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="2dp"
                            android:layout_marginEnd="0dp"
                            android:layout_marginRight="0dp"
                            android:layout_marginBottom="2dp"
                            android:layout_weight="0"
                            android:background="@drawable/ic_baseline_search_24"
                            android:gravity="end"
                            android:padding="0dp"
                            android:visibility="visible" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/LL_top"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0dp"
                    android:gravity="top"
                    android:orientation="vertical"
                    android:padding="0dp"
                    android:weightSum="0">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginBottom="12dp"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="0dp"
                        android:weightSum="2">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.7"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="start"
                            android:padding="2dp"
                            android:text="Booking Date "
                            android:textColor="@color/common_txt_color"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <EditText
                            android:id="@+id/booking_date_tv"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="0dp"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_marginEnd="0dp"
                            android:layout_marginRight="0dp"
                            android:layout_marginBottom="0dp"
                            android:layout_weight="1.3"
                            android:background="@drawable/inputtext_background"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:text=""
                            android:inputType="textCapSentences"
                            android:paddingLeft="5dp"
                            android:textColor="#222c36"
                            android:textColorHint="@color/gray"
                            android:textSize="16dp"
                            android:textStyle="normal"
                            android:visibility="visible">

                        </EditText>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginBottom="12dp"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="0dp"
                        android:weightSum="2">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.7"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="start"
                            android:padding="2dp"
                            android:text="Patient Name "
                            android:textColor="@color/common_txt_color"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <EditText
                            android:id="@+id/patient_name_txt"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="0dp"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_marginEnd="0dp"
                            android:layout_marginRight="0dp"
                            android:layout_marginBottom="0dp"
                            android:layout_weight="1.3"
                            android:background="@drawable/inputtext_background"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:text=""
                            android:inputType="textCapSentences"
                            android:paddingLeft="5dp"
                            android:textColor="#222c36"
                            android:textColorHint="@color/gray"
                            android:textSize="16dp"
                            android:textStyle="normal"
                            android:visibility="visible">

                        </EditText>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginBottom="12dp"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="0dp"
                        android:weightSum="2">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.7"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="start"
                            android:padding="2dp"

                            android:text="Patient UHID "
                            android:textColor="@color/common_txt_color"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <EditText
                            android:id="@+id/patient_id_txt"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="0dp"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_marginEnd="0dp"
                            android:layout_marginRight="0dp"
                            android:layout_marginBottom="0dp"
                            android:layout_weight="1.3"
                            android:background="@drawable/inputtext_background"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:inputType="number"
                            android:paddingLeft="5dp"
                            android:textColor="#222c36"
                            android:maxLength = "10"
                            android:textColorHint="@color/gray"
                            android:textSize="16dp"
                            android:textStyle="normal"
                            android:visibility="visible">

                        </EditText>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginBottom="12dp"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="0dp"
                        android:weightSum="2">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.7"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="start"
                            android:padding="2dp"
                            android:text="Doctor Name"
                            android:textColor="@color/common_txt_color"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <TextView
                            android:id="@+id/doctor_name_txt"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="0dp"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_marginEnd="0dp"
                            android:layout_marginRight="0dp"
                            android:layout_marginBottom="0dp"
                            android:layout_weight="1.3"
                            android:background="@drawable/inputtext_background"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center_vertical"
                            android:hint=""
                            android:inputType="text"
                            android:paddingLeft="5dp"
                            android:textColor="#222c36"
                            android:textColorHint="@color/gray"
                            android:textSize="16dp"
                            android:drawableRight="@drawable/ic_keyboard_arrow_down"
                            android:textStyle="normal"
                            android:visibility="visible">

                        </TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="34dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginBottom="12dp"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="0dp"
                        android:weightSum="2">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.7"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="start"
                            android:padding="2dp"
                            android:text="Schedule Time"
                            android:textColor="@color/common_txt_color"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.3">

                            <Spinner
                                android:id="@+id/schedule_time_spin"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:background="@null"
                                android:spinnerMode="dropdown" />

                            <TextView
                                android:id="@+id/schedule_time_txt"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="0dp"
                                android:layout_marginLeft="0dp"
                                android:layout_marginTop="0dp"
                                android:layout_marginEnd="0dp"
                                android:layout_marginRight="0dp"
                                android:layout_marginBottom="0dp"
                                android:background="@drawable/inputtext_background"
                                android:drawableRight="@drawable/ic_keyboard_arrow_down"
                                android:fontFamily="sans-serif-condensed-light"
                                android:gravity="center_vertical"
                                android:hint=""
                                android:inputType="text"
                                android:paddingLeft="5dp"
                                android:textColor="#222c36"
                                android:textColorHint="@color/gray"
                                android:textSize="16dp"
                                android:textStyle="normal"
                                android:visibility="visible">

                            </TextView>
                        </RelativeLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginEnd="0dp"
                    android:layout_marginBottom="0dp"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:padding="0dp"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginBottom="4dp"
                        android:gravity="start"
                        android:orientation="horizontal"
                        android:padding="0dp"
                        android:weightSum="2">

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="09:00"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="09:30"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="10:00"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="10:30"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginBottom="4dp"
                        android:gravity="start"
                        android:orientation="horizontal"
                        android:padding="0dp"
                        android:weightSum="2">

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="11:00"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="11:30"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="12:00"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="12:30"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="01:00"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginBottom="4dp"
                        android:gravity="start"
                        android:orientation="horizontal"
                        android:padding="0dp"
                        android:weightSum="2">

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="09:00"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="09:30"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="10:00"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />

                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="7dp"
                            android:layout_weight="0"
                            android:background="@drawable/time_bg"
                            android:fontFamily="sans-serif-condensed-light"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="10:30"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="12dp"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="0dp"
                    android:weightSum="2">

                    <EditText
                        android:id="@+id/doctor_note_txt"
                        android:layout_width="match_parent"
                        android:layout_height="160dp"
                        android:layout_marginStart="0dp"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_marginEnd="0dp"
                        android:layout_marginRight="0dp"
                        android:layout_marginBottom="0dp"
                        android:layout_weight="0"
                        android:background="@drawable/inputtext_background"
                        android:fontFamily="sans-serif-condensed-light"
                        android:gravity="start"
                        android:hint="Doctor's Note"
                        android:inputType="textMultiLine|textCapSentences"
                        android:paddingTop="3dp"
                        android:paddingLeft="5dp"
                        android:textColor="#222c36"
                        android:textColorHint="@color/gray"
                        android:textSize="16dp"
                        android:textStyle="normal"
                        android:visibility="visible">

                    </EditText>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="12dp"
                    android:gravity="end"
                    android:orientation="horizontal"
                    android:padding="0dp"
                    android:weightSum="2">

                    <TextView
                        android:id="@+id/save_txt"
                        android:layout_width="80dp"
                        android:layout_height="45dp"
                        android:background="@drawable/time_bg"
                        android:fontFamily="sans-serif-condensed"
                        android:gravity="center"
                        android:padding="0dp"
                        android:text="Save"
                        android:textColor="@color/white"
                        android:textSize="18sp"
                        android:visibility="visible">

                    </TextView>
                </LinearLayout>
            </LinearLayout>

        </androidx.cardview.widget.CardView>
    </LinearLayout>
</LinearLayout>